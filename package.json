{
  "name": "user-service",
  "version": "2.9.0",
  "description": "A microservice for handling user account actions in the CLARK platform.",
  "main": "app.js",
  "scripts": {
    "start": "nodemon",
    "serve": "node -r ts-node/register ./src/app.ts",
    "serve:debug": "node --inspect=5858 -r ts-node/register ./src/app.ts",
    "build": "tsc",
    "lint": "node_modules/.bin/tslint --project ./src/**",
    "pack": "npm run build && cp ./package.json ./dist && cd ./dist && zip -r ../dist.zip  ./* & cd ..",
    "zip": "cp ./package.json ./dist && cd ./dist && zip -r ../dist.zip  ./* & cd ..",
    "deploy": "eb deploy User-Service-Production --profile clark --label $(node -p \"require('./package.json').version\")",
    "prettify": "node_modules/.bin/prettier --write ./src/**/* --single-quote",
    "test": "node_modules/.bin/jest --verbose  --coverage",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cyber4All/user-service.git"
  },
  "author": "Sean Donnelly",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Cyber4All/user-service/issues"
  },
  "homepage": "https://github.com/Cyber4All/user-service#readme",
  "dependencies": {
    "@cyber4all/clark-schema": "^1.0.2",
    "@cyber4all/clark-taxonomy": "^2.0.0",
    "@sendgrid/mail": "^6.2.1",
    "@sentry/node": "^4.4.1",
    "@types/request-promise": "^4.1.44",
    "aws-sdk": "^2.440.0",
    "bcrypt": "^3.0.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.0",
    "express-rate-limit": "^5.1.3",
    "jsonwebtoken": "^8.1.1",
    "mongodb": "^3.1.10",
    "morgan": "^1.9.0",
    "node-fetch": "^2.6.1",
    "node-service-module": "^1.1.0",
    "nyc": "^13.2.0",
    "root-require": "^0.3.1",
    "snyk": "^1.217.3"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.3",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.16.1",
    "@types/express-jwt": "0.0.40",
    "@types/jest": "^23.3.14",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/mongodb": "^3.1.18",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.5.2",
    "@types/supertest": "^2.0.7",
    "danger": "^6.0.2",
    "jest": "^26.6.3",
    "nodemon": "^1.19.1",
    "prettier": "1.14.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.5.2",
    "ts-node": "^8.10.2",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.7.0",
    "typescript": "^4.1.5"
  },
  "prettier": {
    "singleQuote": true
  },
  "snyk": true
}
